/** */
MRdata323.path_expansions = [
	['fe_e_jx',		'frontend-engineering/engineers/javascript/xbrowser'],
	['fe_e_j',		'frontend-engineering/engineers/javascript'],
	['fe_e',		'frontend-engineering/engineers'],
	['fe_k',		'frontend-engineering/knowits'],
	['fe_a',		'frontend-engineering/artists'],
	['fe',			'frontend-engineering']
];

/**I Getters */

/* Four main structures are 'menu_data_arr' (the source array, above) and 'menu_data_arr_on' (above array with added outnums). An 'outnum' is an array of outline numbers. Section 1, subsection 2, detail 3 is outnum [1,2,3].) 'page_data' is an object, keyed by pathname, content: [outnum, topic, tooltip]. 'page_tree' is an object, keyed by comma-joined outnum, content: MenuTreeNode (see class constructor, below).

'menu_data_arr_on' is created when this file is loaded. 'page_data' and 'page_tree' are instantiated on request via 'get_page_data()' and 'get_page_tree()'.

The primary API, called by view to create site menus, is 'get_current_page_menu_data_list()'. This is an array of 'MenuData' objects in home thru current (top thru bottom) menu order.

For the 'page turner' links, get_pcn_menu_data_arr_on() returns three rows from 'menu_data_arr_on': the previous, current and next pages' data.*/

/**A During Initialization */

	/** 'home' and 'page'
	 'home' is the root (in office and online)
	 'page' is the full name of the page after the root (not including "/") */
	MRdata323.home_func = function () {
	/* Returns the location through the first forward slash following, if present,
	'MR23' (or the first slash, if no 'MR23').
	This is correct on the website and on the office computers. */

		var str = window.location.href; // the full location
			str = MRdata323.strip_fragment_identifier( str );

/* Tested: Chrome, Firefox, MSIE, Opera, Safari
	local: prefix starts w/ 'file:', ends w/ 'MR23/'
		P.S. Opera local ends w/ 'MR23//' (two trailing slashes)
	remote: 				'http:',		 'com/' */

		var start = str.substr( 0, 5 );
		var end_loc, path, stop;
		if ( start === 'file:' ) {
			if ( str.toLowerCase().indexOf('mr23//') > -1 ) {
					stop = 'mr23//'; } // Opera, local
			else { stop = 'mr23/'; } // not Opera, local
		}
		if ( start === 'http:' ) { stop = '.com/'; }
		if ( stop === undefined ) { throw "MRdata323.home_func(), "
			"unknown page prefix in " + str; }

		end_loc = str.toLowerCase().indexOf( stop ) + stop.length ;
		MRdata323.home = str.substr( 0, end_loc );
		MRdata323.page = str.substr( end_loc );
		if ( MRdata323.page === '' ) { MRdata323.page = 'index.html'; }

// alert( 'home_func(): ' + window.location.href + '\n\n' + MRdata323.home + '\n' + MRdata323.page );

	} // end: home_func()

/**A As Requested by View */

/** Home -> current. */
MRdata323.get_current_page_menu_data_list = function () {

/*
Initialize empty menu tree.
Start with the page just loaded as the 'current_page'.

Loop:
	Enter the menu data (a MenuData object) for the current page.
	Get the MenuData for the selected page's parent. (Parent and siblings.)
	Enter this MenuData above (as item [0]) the other MenuData in list.
	Until we back up to the menu root (parent key === '(n/a)')
*/

	var menu_tree = []; // array of MenuData objects
	var menu_data = MRdata323.get_current_page_menu_data();
	menu_tree[ 0 ] = menu_data;
	var current_node = menu_data.selected_node();
	if ( current_node.children.length > 0 ) {
		menu_tree[ 1 ] = new MRdata323.MenuData( current_node.children, -1 );
	}

	while (  current_node.parent_key !== '(n/a)' ) {
		menu_data = MRdata323.get_node_menu_data( current_node.parent );
		menu_tree.unshift( menu_data );
		current_node = menu_data.selected_node();
	}

	return menu_tree;

} // end: get_current_page_menu_data_list()

/** current -> end. */
MRdata323.get_current_page_subtree_table = function () {

	var outnum = MRdata323.get_current_page_row()[0];

	var table = new MRlib323.Table( 'Table',
			['outnums', 'topic', 'tooltip', 'name', 'path'],
			MRdata323.menu_data_arr_on );

		table = table.where( 'outnums asw [' + outnum + ']' );

	return table;

} // end: get_current_page_subtree_table()

/** Object, one property per page row. */
MRdata323.get_page_data = function () {

	/* 'page_data' is an object. Its properties are:
		name - path + '/' + name (see below)
		value - [outline numbers, topic, tooltip]

	note: name is contracted path plus filename w/o '.html'
		see MRdata323.path_expansions for list of contractions */

	if ( MRdata323.page_data !== undefined ) { return MRdata323.page_data; }

	MRdata323.page_data = {};
	for ( var i in MRdata323.menu_data_arr_on ) {
		var row = MRdata323.menu_data_arr_on[ i ];
		var path = row[ 4 ];
		if ( path !== '' ) { path += '/'; }
		MRdata323.page_data[ path + row[3] ] = row.slice( 0, 3 );
	}

	return MRdata323.page_data;

} // end: get_page_data()

/** Tree of page row objects. */
MRdata323.get_page_tree = function () {
	if ( MRdata323.page_tree !== undefined ) { return MRdata323.page_tree; }

	var page_tree = {};
	var list = MRdata323.get_page_data();

	for ( var prop in list ) {

		var list_row = list[ prop ]; // see get_page_data()
		// [ outnums, pathname, topic, tooltip ]
		var tree = new MRdata323.MenuTreeNode( list_row[ 0 ], prop,
				list_row[ 1 ], list_row[ 2 ] );
		page_tree[ tree.key ] = tree;

		if ( tree.parent_key !== '(n/a)' ) {
			var parent = page_tree[ tree.parent_key ];
			parent.children.push( tree );
			tree.parent = parent;
		}

	} // end: for prop in list

	MRdata323.page_tree = page_tree;
	return page_tree;

} // end: get_page_tree()

/** Prev, cur, next rows. */
MRdata323.get_pcn_menu_data_arr_on = function () {

	var arr = MRdata323.menu_data_arr_on,
		cp = MRdata323.page;

	cp = MRdata323.contract_path( cp );
	cp = cp.replace( '.html', '' );

	for ( var i in arr ) {
		var row = arr[ i ];
		var path = ( row[4] !== '' ) ? ( row[4] + '/' ) : ''
		if ( cp === (path + row[ 3 ]) ) { return pcn( arr, +i ); }
	}
	throw "No data in MRdata323.menu_data_arr_on for " + MRdata323.page;

	function pcn( arr, i ) { // pcn === previous, current, next

		var prv, cur, nxt;
		prv = i > 0 ? arr[ i - 1 ] : undefined;
		cur = arr[ i ];
		nxt = ( i < (arr.length - 1) ) ? arr[ i + 1 ] : undefined;
		return [ prv, cur, nxt ];

	} // end: pcn()

} // end: find_current_page_menu_data_arr_on()

/**A Private (non-API) Getters */

/** Menu (page w/siblings). */
MRdata323.get_current_page_menu_data = function () {
	var node = MRdata323.get_current_page_tree_node();
	return MRdata323.get_node_menu_data( node );
}

/** One row for the current page.
Returns a row in 'page_data': [ [outnums], topic, tooltip ] */
MRdata323.get_current_page_row = function () {
	var page = MRdata323.page;
		if ( page[0] === '/' ) { page = page.substr( 1 ); }
		page = page.replace( '.html', '' );
		page = MRdata323.contract_path( page );
	var data = MRdata323.get_page_data()[ page ];
	if ( data === undefined ) { throw "In 'get_current_page_row()', no data for " + page; }
	return data;
}

/** Node in menu data tree. */
MRdata323.get_current_page_tree_node = function () {

	var data_row = MRdata323.get_current_page_row();
	var outnums = data_row[ 0 ];
	var tree = MRdata323.get_page_tree();

	var key = ( outnums.length === 0 ) ? '0' : outnums.join( ',' );
	return tree[ key ];

} // */end: get_current_page_tree_node()

/** Menu data (node + siblings ). */
MRdata323.get_node_menu_data = function ( node ) {
	// done quickly if this is the root node:
	if ( !node.parent ) { return new MRdata323.MenuData( [node], 0 ); }

	var parent = node.parent;
	var nodes = parent.children;
	var sel = find( nodes, node );
	return new MRdata323.MenuData( nodes, sel );

	function find( nodes, target ) {
		for ( var i in nodes ) {
			if ( nodes[i] === target ) { return +i; }
		}
		return -1;
	} // end: find()

} // end: get_current_page_menu_data()

/**I Utilities */

/** Full paths abbreviated. */
MRdata323.contract_path = function ( full_path ) {

	var expansions = MRdata323.path_expansions,
		ret;
	for ( var i in expansions ) {
		ret = full_path.replace( expansions[ i ][ 1 ], expansions[ i ][ 0 ] );
		if ( ret !== full_path ) { break; }
	}

	return ret;
} // end: contract_path()

/** Abbreviations spelled out. */
MRdata323.expand_path = function ( abbrev ) {

	var expansions = MRdata323.path_expansions,
		ret;
	for ( var i in expansions ) {
		ret = abbrev.replace( expansions[ i ][ 0 ], expansions[ i ][ 1 ] );
		if ( ret !== abbrev ) { break; }
	}

	return ret;
} // end: expand_path()

/** Load new page. */
MRdata323.go2 = function ( pathname ) {
	pathname = MRdata323.home +
			MRdata323.expand_path( pathname ) + '.html'
	window.location.href = pathname;
} // end: go2()

/** Remove frag id from URL */
MRdata323.strip_fragment_identifier = function (full_pathname) {

	// full pathname may be a URL object (from window.location)
	// and may include #frag_id ("../hither/yon/xxx.html#fragment_id")

	/* 	(			start, $1
			[^#]		any character except '#'
			+			1 or more (non '#' chars)
		)			end, $1
		(			start, $2
			#			the '#' character
			.*			zero or more, any character
		)			end, $2
	*/
	var has_frag_id = ('' + full_pathname).match(  /([^#]+)(#.*)/  );
	// true if there is a '#'
		// $1: all chars before the first (optional) '#'
		// $2: the first (optional) '#' and all following chars

	return has_frag_id ? RegExp.$1 : full_pathname;
}

/**I Classes */

/**A MenuTreeNode */

/** */
MRdata323.MenuTreeNode = function ( outnums, pathname, topic, tooltip ) {
	this.outnums = outnums;
	this.pathname = pathname;
	this.topic = topic;
	this.tooltip = tooltip;
	this.key = make_key( this.outnums );
	this.parent_key = make_parent_key( this.outnums );
	this.parent = null;
	this.children = [];

	function make_key( outnums ) {
		if ( outnums.length === 0 ) { return '0'; }
		return outnums.join( ',' );
	}

	function make_parent_key( outnums ) {
		if ( outnums.length === 0 ) { return '(n/a)'; }
		if ( outnums.length === 1 ) { return '0'; }
		return outnums.slice( 0, outnums.length - 1 ).join( ',' );
	} // end: make_parent_key()

} // end: MenuTreeNode()

/** */
MRdata323.MenuTreeNode.prototype.toString = function () {
	return 'MenuTreeNode{' +
		'outnums=' + this.outnums +
		',pathname=' + this.pathname +
		',topic=' + this.topic +
		',tooltip=' + this.tooltip +
		',key=' + this.key +
		',parent_key=' + this.parent_key +
		',# of children=' + this.children.length + '}';
} // end: MenuTreeNode.toString()

/**A MenuData */

/** */
MRdata323.MenuData = function ( menu_tree_nodes, index_selected ) {
	if ( index_selected === undefined ) { index_selected = -1; }
	this.menu_tree_nodes = menu_tree_nodes;
	this.selected = index_selected;
}

/** */
MRdata323.MenuData.prototype.selected_node = function () {
		return this.menu_tree_nodes[ this.selected ]; }

/** */
MRdata323.MenuData.prototype.toString = function () {
	var ret = 'MenuData{';
		ret += 'selected=' + this.selected;
	for ( var i in this.menu_tree_nodes ) {
			ret += ',\r\n' + this.menu_tree_nodes[ i ]; }
	ret += '}';
	return ret;
} // end: MenuData.toString()

/**I Page Turners */

/**A Page Turner Data */

/** Styles, Both */
MRdata323.turner_styles = {
	background:		'#f8f4f0',
	border:			'5px solid #d8d4d0',
	borderRadius:	'10px',
	fontSize:		'10pt',
	fontWeight:		'bold',
	height:			'20px',
	lineHeight:		'20px',
	opacity:		'0.5',
	paddingLeft:	'1px',
	position:		'fixed',
	width:			'35px',
	zIndex:			99
}

MRdata323.turner_styles_prev = {
	bottom:			'10px',
	left:			'10px'
}

MRdata323.turner_styles_next = {
	bottom:			'10px',
	right:			'10px'
}

/**A Code */ // Note: standalone code, no MRlib support
MRdata323.show_turners = function () {

	// get prev, current, next data arrays
	var pcn = MRdata323.get_pcn_menu_data_arr_on();

	// construct page URLs from [... filename, abbreviated path]
	var prev_page = undefined;
	if ( pcn[0] !== undefined ) {
		prev_page = fixup_url( pcn[0] );
	}
	var next_page = undefined;
	if ( pcn[2] !== undefined ) {
		next_page = fixup_url( pcn[2] );
	}

	var prev = make_turner( MRdata323.turner_styles_prev );
		prev.innerHTML = make_link( prev_page, 'PREV' );
	var next = make_turner( MRdata323.turner_styles_next );
		next.innerHTML = make_link( next_page, 'NEXT' );

	function fixup_url( arr_on_row ) { // data w/outnums

		var ret = arr_on_row[ 3 ]; // partial pathname
		if ( arr_on_row[4] !== '' ) {
			var path = MRdata323.expand_path( arr_on_row[4] );
			ret = path + '/' + ret;
		}
		return MRdata323.home + ret + '.html';

	} // end: fixup_url()

	function make_link( url, label ) { // use HTML
		if ( url !== undefined ) {
			return '<a href="' + url + '" style="text-decoration: none">' +
					label + '</a>';
		}
		return '';
	}

	function make_turner( styles ) {
		var ret = document.createElement( 'div' );
		style( ret, MRdata323.turner_styles ); // styles for both
		style( ret, styles ); // styles for 'prev' or 'next'
		document.body.appendChild( ret );
		return ret;
	}

	function style( elem, styles ) {
		for ( var prop in styles ) { elem.style[ prop ] = styles[ prop ]; }
	}

} // end: show_turners()

/**I MRdata323 Initialization */

/**A Root (xxx.home) and Current Page (xxx.page) */
MRdata323.home_func(); // look up root, current page pathname

/**A Outline Numbers (xxx.menu_data_arr_on) */
MRdata323.menu_data_arr_on = MRlib323.outnum( MRdata323.menu_data_arr );
/* Data with added outline numbers. Columns are:
	0 - array of outline numbers
	1 - topic, leading tabs stripped
	2 - tooltip
	3, 4 - page name, path (as above ) */

MRdata323.show_turners();

// console.log( 'mr-data323.js loaded' );

/* end mr-data323.js */
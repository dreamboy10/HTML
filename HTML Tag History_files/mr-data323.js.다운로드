*/

'use strict';

/**I Data */

/** The Global Data Object */
var MRdata323 = {};

/** One entry for each page. */
MRdata323.menu_data_arr = [

/* Entries are four item arrays:
	e[0]	optional tabs (note below) plus menu text
	e[1]	tooltip text
	e[2]	pathname without prefix (see e[3]) and without '.html' suffix
	e[3]	path prefix contraction (listed in MRdata323.path_expansions)
				example: 'fe_e' expansion 	['fe_e',		'frontend-engineering/engineers'],

	Optional tabs:
		none?	root item, home page (must be first item)

		let p# = previous item's # of tabs
		let c# = current item's # of tabs

		c# === p#		sibling of previous item
		c# === p# + 1	first child of previous item
		c# < p#			sibling of first item above with # tabs === c#
*/

